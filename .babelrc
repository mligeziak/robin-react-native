{
  "presets": ["babel-preset-expo", "react-native-dotenv"],
  "env": {
    "development": {
      "plugins": ["transform-react-jsx-source"]
    }
  },
  "plugins": [
    [
      "module-resolver",
      {
        "alias": {
          "components": "./src/components",
          "utils": "./src/utils",
          "containers": "./src/containers",
          "static": "./static"
        }
      }
    ]
  ]
}
